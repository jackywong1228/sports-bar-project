.container { min-height: 100vh; background: #f5f5f5; }
.header { background: linear-gradient(135deg, #FF6B35, #FF8F65); padding: 60rpx 30rpx; color: #fff; }
.level { font-size: 40rpx; font-weight: bold; display: block; margin-bottom: 12rpx; }
.expire { font-size: 26rpx; opacity: 0.8; }
.section { background: #fff; padding: 30rpx; margin-top: 20rpx; }
.title { font-size: 32rpx; font-weight: 500; margin-bottom: 24rpx; }
.card { display: flex; justify-content: space-between; padding: 24rpx; background: #f5f5f5; border-radius: 12rpx; margin-bottom: 16rpx; }
.card-name { font-size: 30rpx; }
.card-price { font-size: 32rpx; color: #FF6B35; font-weight: bold; }
.empty { text-align: center; padding: 40rpx; color: #999; }
